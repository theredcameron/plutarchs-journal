	                  <script>
headerID = document.getElementById('header-donate');

var headerScroll = function () {
    var y = window.scrollY;
    if (y >= 20) {
        headerID.className = 'header-donate-mobile hideheader';
    } else {
        headerID.className = 'header-donate-mobile showheader';
    }
};

window.addEventListener('scroll', headerScroll);

menuID = document.getElementById('mobile-menu-id');

var menuAdj = function () {
    var y = window.scrollY;
    if (y >= 20) {
        menuID.className = 'mobile-menu widemenu';
    } else {
        menuID.className = 'mobile-menu shortmenu';
    }
};

window.addEventListener('scroll', menuAdj);
</script>
